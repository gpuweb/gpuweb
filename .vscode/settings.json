{
    "editor.rulers": [100],
    "cSpell.words": [
        "localizable",
        "uncategorized",
        "workgroups",
    ],
    "cSpell.enabled": true,
    "files.associations": {
        "*.bs.include": "bikeshed",
    },
    "files.eol": "\n",
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "[bikeshed]": {
        "editor.detectIndentation": false,
        "editor.indentSize": 4,
    },
}
