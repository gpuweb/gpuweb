{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "wgsl",
      "camelcase": "Wgsl",
      "title": "wgsl",
      "scope": "source.wgsl",
      "file-types": [
        "wgsl"
      ],
      "injection-regex": "^wgsl$",
      "class-name": "TreeSitterWgsl"
    }
  ],
  "metadata": {
    "version": "0.0.8",
    "license": "BSD-3-Clause",
    "description": "WGSL grammar for tree-sitter",
    "links": {
      "repository": "https://github.com/gpuweb/tree-sitter-wgsl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
